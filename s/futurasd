local laser
local laser_wel
local laser_mesh
local laser_att
local pistoll
local wel
local pistol_mesh
local swordd
local wel2
local sword_mesh
function change_clothes(s)
	if s == 'shirt' then
		local num
		if clothes.shirt.current == #clothes.shirt.colors then clothes.shirt.current = 1
		else
			clothes.shirt.current = clothes.shirt.current + 1
		end
		local colr = clothes.shirt.colors[clothes.shirt.current]
		if laser_mesh then
			laser_mesh.VertexColor = Vector3.new(colr[1], colr[2], colr[3])
		end
		for i,v in ipairs(clothes.shirt.parts) do
			v.Color = Color3.new(colr[1], colr[2], colr[3])
		end
	elseif s == 'pants' then
		local num
		if clothes.pants.current == #clothes.pants.colors then clothes.pants.current = 1
		else
			clothes.pants.current = clothes.pants.current + 1
		end
		local colr = clothes.pants.colors[clothes.pants.current]
		if laser_mesh then
			laser_mesh.VertexColor = Vector3.new(colr[1], colr[2], colr[3])
		end
		for i,v in ipairs(clothes.pants.parts) do
			v.Color = Color3.new(colr[1], colr[2], colr[3])
		end
	end
end
function toggle_pistol()
	if not pistoll then
		if left_hand.Parent == nil then return end
		pistoll = Instance.new("Part", left_hand)
		wel = Instance.new('Weld', pistoll)
		pistol_mesh = Instance.new('SpecialMesh', pistoll)
		pistoll.Size = Vector3.new(1,1,1)
		pistoll.CanCollide = false
		wel.Part0 = pistoll
		wel.Part1 = left_hand
		wel.C0 = CFrame.new(0,-0.4,-1.3) * CFrame.Angles(math.rad(270), math.rad(180), 0)
		pistol_mesh.MeshType = 'FileMesh'
		pistol_mesh.MeshId = 'rbxassetid://436697873'
		pistol_mesh.TextureId = 'rbxassetid://436697889'
		pistol_mesh.Scale = Vector3.new(0.05, 0.05, 0.05)
	else
		if pistoll then
			pistoll:Destroy()
		end
		pistoll = nil
		pistol_mesh = nil
		wel = nil
	end
end
function toggle_laser()
	if not laser then
		if left_hand.Parent == nil then return end
		laser = Instance.new("Part", left_hand)
		laser_wel = Instance.new('Weld', laser)
		laser_mesh = Instance.new('SpecialMesh', laser)
		laser_att = Instance.new('Attachment', laser)
		laser_att.Position = Vector3.new(0,0.27,-1.24)
		laser.Size = Vector3.new(0.58, 1.34, 2.48)
		laser.CanCollide = false
		laser_mesh.MeshType = 'FileMesh'
		laser_mesh.MeshId = 'rbxassetid://130099641'
		laser_mesh.TextureId = 'rbxassetid://3438326153'
		laser_mesh.Scale = Vector3.new(0.65, 0.65, 0.65)
		local colr = clothes.shirt.colors[clothes.shirt.current]
		laser_mesh.VertexColor = Vector3.new(colr[1], colr[2], colr[3])
		laser_wel.Part0 = laser
		laser_wel.Part1 = left_hand
		laser_wel.C0 = CFrame.new(0,0,1.24) * CFrame.Angles(math.rad(90),0,0)
	else
		if laser then
			laser:Destroy()
		end
		laser = nil
		laser_mesh = nil
		laser_wel = nil
		laser_att = nil
	end
end
local dmg = false
local ts = game:GetService('TweenService')
function ctween(prt, info, goal)
local info = TweenInfo.new(info[1],info[2],info[3],info[4],info[5],info[6])
local tw = ts:Create(prt,info,goal)
tw:Play()
return tw
--example: ctween(part, {0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0}, {CFrame = CFrame.new(0, 10, 0)})
end
function toggle_sword()
	if not swordd then
		if right_hand.Parent == nil then return end
		swordd = Instance.new("Part", right_hand)
		wel2 = Instance.new('Weld', swordd)
		sword_mesh = Instance.new('SpecialMesh', swordd)
		swordd.Size = Vector3.new(0.4, 1, 9)
		swordd.CanCollide = false
		wel2.Part0 = swordd
		wel2.Part1 = right_hand
		wel2.C0 = CFrame.new(0, 0, 4.5) * CFrame.Angles(math.rad(0), 0, 0)
		sword_mesh.MeshType = 'FileMesh'
		sword_mesh.MeshId = 'http://www.roblox.com/asset/?id=365565135'
		sword_mesh.TextureId = 'http://www.roblox.com/asset/?id=365565061'
		sword_mesh.Scale = Vector3.new(4,4,4)
		swordd.Touched:Connect(function(p)
			if dmg == true then
				if p:FindFirstAncestorOfClass'Model' then
					if p:FindFirstAncestorOfClass'Model' ~= owner.Character then
						if p.Size.X < 40 and p.Size.Y < 40 and p.Size.Z < 40 then
							p:Destroy()	
						end
					end
				else
				if p.Size.X < 40 and p.Size.Y < 40 and p.Size.Z < 40 then
					p:Destroy()	
				end
				end
			end
		end)
	else
		if swordd then
			swordd:Destroy()
		end
		swordd = nil
		sword_mesh = nil
		wel2 = nil
	end
end
local dontshoot = false
local dontshoot_l = false
function shoot(fat, pos)
	if dontshoot == true then return end
	if not pistoll then return end
	if pistoll.Parent == nil then return end
	local s = Instance.new('Sound', pistoll)
	s.Volume = 5
	s.SoundId = 'rbxassetid://356911785'
	s:Play()
	dontshoot = true
	coroutine.wrap(function()
		wait(0.4)
		dontshoot = false
	end)()
	if (pistoll.CFrame.p - fat.CFrame.p).magnitude < 200 then
	fat:BreakJoints()
	local exp = Instance.new('Explosion', pistoll)
	exp.Position = pos
	end
end
function l_shoot(fat, pos)
	if dontshoot_l == true then return end
	if not laser then return end
	if laser.Parent == nil then return end
	local s = Instance.new('Sound', laser)
	s.Volume = 5
	s.SoundId = 'rbxassetid://151414336'
	s:Play()
	dontshoot_l = true
	coroutine.wrap(function()
		wait(0.1)
		dontshoot_l = false
	end)()
	local p = Instance.new('Part', laser)
	p.Transparency = 1
	p.Size = Vector3.new(0,0,0)
	p.Anchored = true
	p.CanCollide = false
	p.Position = pos
	local attac = Instance.new('Attachment', p)
	local beam = Instance.new('Beam', laser)
	beam.Attachment0 = laser_att
	beam.Attachment1 = attac
	beam.FaceCamera = true
	beam.LightEmission = 1
	beam.Transparency = NumberSequence.new(0)
	beam.Width0 = 0.3
	beam.Width1 = 0.3
	local colr = clothes.shirt.colors[clothes.shirt.current]
	beam.Color = ColorSequence.new(Color3.new(colr[1], colr[2], colr[3]))
	game:GetService('Debris'):AddItem(beam, 0.1)
	game:GetService('Debris'):AddItem(p, 0.1)
	if (laser.CFrame.p - fat.CFrame.p).magnitude < 2000 then
	if fat.Parent:IsA'Model' and fat.Parent ~= handsome_man and fat.Parent ~= workspace then
		for i,v in ipairs(fat.Parent:GetDescendants()) do
			pcall(function()
			if v.Size.X < 40 and v.Size.Y < 40 and v.Size.Z < 40 then
			v.Material = 'Glass'
			v.Color = Color3.new(colr[1], colr[2], colr[3])
			v.Anchored = true
			v:BreakJoints()
			ctween(v, {1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0}, {Transparency = 1, CFrame = v.CFrame * CFrame.new(0,2,0), Size = Vector3.new(0,0,0)})
			game:GetService'Debris':AddItem(v, 1)
			end
			end)
			if v:IsA'Humanoid' then
				v.Health = 0
			end
		end
	else
		if fat.Size.X < 40 and fat.Size.Y < 40 and fat.Size.Z < 40 then
		fat.Material = 'Glass'
		fat.Color = Color3.new(colr[1], colr[2], colr[3])
		fat.Anchored = true
		fat:BreakJoints()
		ctween(fat, {1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0}, {Transparency = 1, CFrame = fat.CFrame * CFrame.new(0,2,0),Size = Vector3.new(0,0,0)})
		game:GetService'Debris':AddItem(fat, 1)
		end
	end
	end
end
function swing()
	coroutine.wrap(function()
	dmg = true
	wel2.C0 = CFrame.new(0, 0, 4.5) * CFrame.Angles(math.rad(90), 0, 0)
	wait(0.2)
	dmg = false
	wel2.C0 = CFrame.new(0, 0, 4.5) * CFrame.Angles(math.rad(0), 0, 0)
	end)()
end

if bc then
	left_hand.Color = bc.LeftArmColor3
end
