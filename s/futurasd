local bc
local oldface
local oldc
local m
local hats = {}
if owner.Character:FindFirstChildOfClass'BodyColors' then
bc = owner.Character:FindFirstChildOfClass'BodyColors'
bc.Parent = nil
end
for i,v in ipairs(owner.Character:GetDescendants()) do
	if v:IsA'Decal' and v.Name == 'face' then
		oldface = v
	end
	if v:IsA'Accessory' then
		table.insert(hats, {v:WaitForChild'Handle', v.AttachmentPos})
		v:WaitForChild'Handle'.Parent = nil
		v:Destroy()
	end
end
if owner.Character:FindFirstChild'Head' then
	if owner.Character:FindFirstChild'Head':FindFirstChildOfClass'SpecialMesh' then
		m = owner.Character:FindFirstChild'Head':FindFirstChildOfClass'SpecialMesh'
		m.Parent = nil
	end
	
end
if owner.Character:FindFirstChildOfClass'Humanoid' then
	oldc = owner.Character:FindFirstChildOfClass'Humanoid'.RootPart.CFrame
end
owner.Character = nil
