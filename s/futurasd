hat_att.CanCollide = false
hat_att.Transparency = 1
hat_att.Size = Vector3.new(2,2,2)
hat_att.name = 'Hats'
local weld_hat = Instance.new('Weld', hat_att)
weld_hat.Part0 = hat_att
weld_hat.Part1 = head
weld_hat.C0 = CFrame.new(0,-0.73,0)
for i,p in ipairs(hats) do
local v = p[1]
local mult = 1.65
v.Mesh.Scale = v.Mesh.Scale * Vector3.new(mult,mult,mult)
v.Parent = hat_att
v.CanCollide = false
local w = Instance.new('Weld', v)
w.Part0 = v
w.Part1 = hat_att
w.C0 = CFrame.new(p[2].X * mult, p[2].Y * mult, p[2].Z * mult)
w.Name = 'AccessoryWeld'
for i,v in ipairs(v:GetDescendants()) do
	if v:IsA'TouchTransmitter' then 
		v:Destroy()
	end
end
v.DescendantAdded:Connect(function(p)
	if p:IsA'TouchTransmitter' then
		p:Destroy()
	end
end)
