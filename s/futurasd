owner:LoadCharacter()

owner.CharacterAdded:connect(function()
pcall(function()
local asd = Instance.new("Seat",owner.Character.Head)
asd.Position = owner.Character.Head.Position
asd.Anchored = false
asd.CanCollide = false
asd.Transparency = 1


local Weld = Instance.new("Weld", asd)
Weld.Part0 = owner.Character.Head
Weld.Part1 = asd
Weld.C1 = CFrame.new(0,2,-1.5)
end)
end)

local kills = math.random(0, 6)
if not owner then
	wait(2)
	owner = game.Players.tratoi
	wait(.5)
end
local saves = {
left = {};
right = {};
}
local remotes = Instance.new("Folder",script)
remotes.Name = "Remotes"

local grab = Instance.new("RemoteEvent",remotes)
local reset = Instance.new("RemoteEvent",remotes)
local drop = Instance.new("RemoteEvent",remotes)
drop.Name = "Drop"
grab.Name = "Grab"
reset.Name = "Reset"

reset.OnServerEvent:Connect(function(guy)
	guy:LoadCharacter(true)
end)

function dropp(plr, arm)
local character = plr.Character
local rightHand = character["Right Hand"]
local leftHand = character["Left Hand"]
	if arm == "Left" then
		if leftHand:FindFirstChild("ServerGrip") then
			if leftHand:FindFirstChild("ServerGrip").Part0 then
				local hit = leftHand:FindFirstChild("ServerGrip").Part0
				for i,v in ipairs(saves.left) do
					v[1].Massless = v[2]
					v[1].Anchored = v[3]
					v[1].CanCollide = v[4]
				end
				saves.left = {}
				if hit:FindFirstAncestorOfClass'Model' then
					for i,v in ipairs(hit:FindFirstAncestorOfClass'Model':GetDescendants()) do
						pcall(function()
							v.PlatformStand = false
						end)
					end
				end	
			end
			leftHand:FindFirstChild("ServerGrip"):Destroy()
		end
	end
	if arm == "Right" then
		if rightHand:FindFirstChild("ServerGrip") then
			if rightHand:FindFirstChild("ServerGrip").Part0 then
				local hit = rightHand:FindFirstChild("ServerGrip").Part0
				for i,v in ipairs(saves.right) do
					v[1].Massless = v[2]
					v[1].Anchored = v[3]
					v[1].CanCollide = v[4]
				end
				saves.right = {}
				if hit:FindFirstAncestorOfClass'Model' then
					for i,v in ipairs(hit:FindFirstAncestorOfClass'Model':GetDescendants()) do
						pcall(function()
							v.PlatformStand = false
						end)
					end
				end	
			end
			rightHand:FindFirstChild("ServerGrip"):Destroy()
		end
	end
end

drop.OnServerEvent:Connect(function(plr,arm)
	dropp(plr, arm)
end)

function grabebe(plr, hit, offset, arm)
local character = plr.Character
local rightHand = character["Right Hand"]
local leftHand = character["Left Hand"]
if hit == 'pistol' then
	if kills >=5 then
		toggle_laser()
	else
		toggle_pistol()
	end
return end
if hit == 'shirt' then
	change_clothes('shirt')
return end
if hit == 'pants' then
	change_clothes('pants')
end
if hit == 'shoot' then
	if kills >=5 then
		l_shoot(offset, arm)
	else
		shoot(offset, arm)
	end
return end
if hit == 'sword' then
	toggle_sword()
return end
if hit == 'swing' then
	swing()
return end
if hit == 'anchor' then
print('anchor')
if offset == 'left' then
	print'left'
	if leftHand:FindFirstChild'ServerGrip' then
		local ler = leftHand:FindFirstChild'ServerGrip'.Part0
		ler.CanCollide = not ler.Anchored
		ler.Anchored = not ler.Anchored
	end
elseif offset == 'right' then
	print'right'
	if rightHand:FindFirstChild'ServerGrip' then
		local ler = rightHand:FindFirstChild'ServerGrip'.Part0
		ler.CanCollide = not ler.Anchored
		ler.Anchored = not ler.Anchored
	end
end
return end
if arm == "Left" then
	local motor = Instance.new("Motor6D")
	motor.Name = "ServerGrip"
	motor.Part0 = hit
	motor.Part1 = leftHand
	motor.C0 = offset
	motor.Parent = leftHand
	table.insert(saves.left, {hit, hit.Massless, hit.Anchored, hit.CanCollide})
	hit.Massless = true
	if hit:FindFirstAncestorOfClass'Model' then
		for i,v in ipairs(hit:FindFirstAncestorOfClass'Model':GetDescendants()) do
			pcall(function()
			table.insert(saves.left, {v, v.Massless, v.Anchored, v.CanCollide})
			end)
			pcall(function()
				v.PlatformStand = true
			end)
			pcall(function()
				v.Massless = true
			end)
		end
	end
end
if arm == "Right" then
	local motor = Instance.new("Motor6D")
	motor.Name = "ServerGrip"
	motor.Part0 = hit
	motor.Part1 = rightHand
	motor.C0 = offset
	motor.Parent = rightHand
	table.insert(saves.right, {hit, hit.Massless, hit.Anchored, hit.CanCollide})
	hit.Massless = true
	if hit:FindFirstAncestorOfClass'Model' then
		for i,v in ipairs(hit:FindFirstAncestorOfClass'Model':GetDescendants()) do
			pcall(function()
			table.insert(saves.right, {v, v.Massless, v.Anchored, v.CanCollide})
			end)
			pcall(function()
				v.PlatformStand = true
			end)
			pcall(function()
				v.Massless = true
			end)
		end
	end
end
end
grab.OnServerEvent:Connect(function(plr,hit,offset,arm)
grabebe(plr, hit, offset, arm)
end)



local added = Instance.new("RemoteEvent",owner)
added.Name = "added"



